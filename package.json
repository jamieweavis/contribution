{
  "name": "contribution",
  "description": "GitHub contribution graph parser - contribution streak & statistic calculator with zero dependencies",
  "version": "7.1.1",
  "author": "Jamie Weavis",
  "types": "lib/contribution.d.ts",
  "source": "src/contribution.ts",
  "main": "lib/contribution.js",
  "homepage": "https://github.com/jamieweavis/contribution#contribution",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamieweavis/contribution/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "contribution",
    "contribution-graph",
    "github",
    "stats",
    "statistics",
    "streak"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jamieweavis/contribution.git"
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "jest --testMatch '/**/*.spec.ts'",
    "test:watch": "pnpm test --watch",
    "check": "biome check ./src",
    "check:write": "pnpm check --write"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@parcel/packager-ts": "2.16.0",
    "@parcel/transformer-typescript-types": "2.16.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.6",
    "@semantic-release/npm": "12.0.2",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@types/jest": "30.0.0",
    "@types/node": "22.14.0",
    "jest": "30.1.3",
    "parcel": "2.16.0",
    "ts-jest": "29.4.4",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/changelog",
      "@semantic-release/commit-analyzer",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github",
      "@semantic-release/release-notes-generator"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}

{
  "name": "contribution",
  "description": "GitHub contribution graph parser - contribution streak & statistic calculator with zero dependencies",
  "version": "7.1.2",
  "author": "Jamie Weavis",
  "source": "src/main.ts",
  "types": "dist/main.d.ts",
  "main": "dist/main.js",
  "homepage": "https://github.com/jamieweavis/contribution#readme",
  "license": "MIT",
  "repository": "github:jamieweavis/contribution",
  "bugs": {
    "url": "https://github.com/jamieweavis/contribution/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "contribution",
    "contributions",
    "contribution-graph",
    "github",
    "statistics",
    "stats",
    "streak",
    "parser"
  ],
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "biome check",
    "check:fix": "biome check --fix"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@parcel/packager-ts": "2.16.0",
    "@parcel/transformer-typescript-types": "2.16.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.6",
    "@semantic-release/npm": "12.0.2",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@types/node": "22.18.11",
    "@vitest/coverage-v8": "3.2.4",
    "parcel": "2.16.0",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/changelog",
      "@semantic-release/commit-analyzer",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github",
      "@semantic-release/release-notes-generator"
    ]
  }
}

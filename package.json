{
  "name": "contribution",
  "description": "GitHub contribution streak & stat fetcher with zero dependencies",
  "version": "5.3.0",
  "author": "Jamie Weavis",
  "types": "lib/contribution.d.ts",
  "source": "src/contribution.ts",
  "main": "lib/contribution.js",
  "homepage": "https://github.com/jamieweavis/contribution#contribution",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamieweavis/contribution/issues"
  },
  "files": ["lib/**/*"],
  "keywords": [
    "contribution",
    "contribution-graph",
    "fetcher",
    "github",
    "no-dependencies",
    "stat",
    "statistics",
    "streak"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jamieweavis/contribution.git"
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "jest --testMatch '/**/*.spec.ts'",
    "test:watch": "npm run test -- --watch",
    "lint": "biome lint ./{src,tests}",
    "lint:fix": "npm run lint -- --write",
    "format": "biome format ./{src,tests}",
    "format:write": "npm run format --  --write",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@parcel/packager-ts": "^2.13.3",
    "@parcel/transformer-typescript-types": "^2.13.3",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.1",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.5",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "lint-staged": "15.4.3",
    "parcel": "^2.13.3",
    "semantic-release": "24.2.3",
    "ts-jest": "29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "5.7.3"
  }
}

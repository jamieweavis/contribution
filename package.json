{
  "name": "contribution",
  "description": "GitHub contribution streak & stat fetcher with zero dependencies",
  "version": "4.1.1",
  "author": "Jamie Weavis",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "homepage": "https://github.com/jamieweavis/contribution",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamieweavis/contribution/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "contribution",
    "contribution-graph",
    "fetcher",
    "github",
    "no-dependencies",
    "stat",
    "statistics",
    "streak"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jamieweavis/contribution.git"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/npm": "7.0.9",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/jest": "29.2.3",
    "@types/node": "14.14.10",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "codecov": "3.8.1",
    "commitizen": "4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.2.0",
    "husky": "4.3.4",
    "jest": "29.3.1",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "semantic-release": "17.3.0",
    "ts-jest": "29.0.3",
    "typescript": "3.9.7"
  },
  "scripts": {
    "coverage": "codecov",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'",
    "test": "jest",
    "build": "tsc",
    "dev": "tsc -w"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
